<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="pembelian_actions_id" model="ir.actions.act_window">
        <field name="name">Pembelian</field>
        <field name="res_model">algoritma.pembelian/</field>
    </record>

    <record id="pembelian_actions_tree_view_id" model="ir.actions.act.window.view">
        <field name="sequence" eval="1"/>
        <field name="view_mode">tree</field>
        <field name="view_id"  ref="algoritma_pembelian_tree_view_id"/>
        <field name="act_window_id" ref="pembelian_actions_id"/>
    </record>

</odoo>